<p id="notice"><%= notice %></p>

<h1>Listing Tipe Simpanans</h1>

<table>
  <thead>
    <tr>
      <th>Ident name</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipe_simpanans.each do |tipe_simpanan| %>
      <tr>
        <td><%= tipe_simpanan.ident_name %></td>
        <td><%= tipe_simpanan.name %></td>
        <td><%= link_to 'Show', tipe_simpanan %></td>
        <td><%= link_to 'Edit', edit_tipe_simpanan_path(tipe_simpanan) %></td>
        <td><%= link_to 'Destroy', tipe_simpanan, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipe simpanan', new_tipe_simpanan_path %>
